@using EM.Model.VMs;
@model List<ExpenseAccountListVM>
<table class="table table-hover">
    <thead>
        <tr>
            <th class="w50">单号</th>
            <th class="w150">申请人</th>
            <th class="w80">公司名称</th>
            <th>金额</th>
            <th>原因</th>
            <th>报销时间</th>
            <th>录入时间</th>
            <th>创建时间</th>
            <th>备注</th>
            <th>操作</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var EA in Model)
        {
            <tr>
                <td>
                    @EA.EANumber
                </td>
                <td>
                    @EA.Name
                </td>
                <td>
                    @EA.CompanyName
                </td>
                <td>
                    @EA.Money
                </td>
                <td>
                    @EA.CateName
                </td>
                <td>
                    @EA.OccurDate
                </td>
                <td>
                    @EA.ApplyDate
                </td>
                <td>
                    @EA.CreateDate
                </td>
                <td>
                    @EA.Remark
                </td>
                <td>
                    <a data-commond="edit" onclick="window.parent.Global.Iframe.OpenIframe('编辑单据_@(EA.EANumber)','EditExpenseAccount_@(EA.Id)','@Url.Action("Edit", new { Id=EA.Id })',false)">编辑</a>
                    <a data-commond="delete" onclick="Global.Form.Delete('@Url.Action("Delete", new {Id=EA.Id })',this,false)">删除</a>
                </td>
            </tr>
        }
    </tbody>
</table>
