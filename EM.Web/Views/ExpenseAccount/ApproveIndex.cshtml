@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutCommon.cshtml";
}
@using EM.Model.VMs
@using EM.Common;
@using EM.Data.Infrastructure;
@model PagedResult<ExpenseAccountListVM>
<div class="SearchFrom">
    <form id="SearchForm" action="@Url.Action("ApproveIndex", "ExpenseAccount")">
        <input type="text" name="EANumber" value="" placeholder="单号">
        <input type="text" name="Name" value="" placeholder="报销人">
        <input type="text" name="CompanyID" value="" placeholder="公司名称">
        @Html.DropDownList("CateId", (SelectList)ViewBag.CateList, "请选择")
        @Html.DropDownList("CompanyId", (SelectList)ViewBag.CompanyList, "请选择")
        @Html.DropDownList("DateType", (SelectList)ViewBag.DateTypeList, "请选择")
        @Html.DropDownList("ApproveStatus", (SelectList)ViewBag.ApproveStatusList, "请选择")
        <input type="text" class="datepicker" placeholder="开始时间" name="SDate" />
        <input type="text" class="datepicker" placeholder="结束时间" name="EDate" />
        <button class="searchButton" type="submit">搜索</button>
    </form>

</div>

<div id="List">
    @Html.Partial("_ApproveList", Model)
</div>
